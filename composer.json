{
  "name": "getsidekicker/flagr-client",
  "description": "Flagr is a feature flagging, A/B testing and dynamic configuration microservice. The base path for all the APIs is \"/api/v1\".",
  "keywords": [
    "flagr",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://openapi-generator.tech",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "rector/rector": "^0.11.47"
  },
  "autoload": {
    "psr-4": {
      "Flagr\\Client\\": "lib/"
    }
  },
  "scripts": {
    "fixer": "vendor/bin/php-cs-fixer fix --config=./.php_cs.php --using-cache=no",
    "rector": "vendor/bin/rector process lib"
  }
}
